<%= form_for [@user, @album] do |f| %>
  <div>
    <div>
      <%= f.label :title %>
      <%= f.text_field :title %>
    </div>
    <div>
      <%= f.label :description %>
      <%= f.text_field :description %>
    </div>
  </div>

  <%= f.submit "Add Album" %>
<% end %>

<%= link_to 'Back to Profile', user_path(@user) %>
