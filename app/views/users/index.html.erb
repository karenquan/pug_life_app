<div class="container">
  <h1>Users</h1>
  <table>
    <tr>
      <td>ID</td>
      <td>First Name</td>
      <td>Last Name</td>
      <td>Username</td>
      <td>Is Admin</td>
    </tr>
    <% @users.each do |user| %>
    <tr>
      <td><%= user.id %></td>
      <td><%= user.first_name %></td>
      <td><%= user.last_name %></td>
      <td><%= user.username %></td>
      <td><%= user.is_admin %></td>
    </tr>
    <% end %>
  </table>
</div>
